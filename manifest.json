{
  "manifest_version": 2,
  "name": "Twitch Private Browser Watch",
  "version": "1.0",
  "author": "Plamen Kinov",
  "description": "Remove some elements from Twitch.tv when open in private browser.",
  "content_scripts": [
    {
      "matches": [
        "*://*.twitch.tv/*"
      ],
      "js": [
        "remove-elemets.js"
      ]
    }
  ],
  "browser_specific_settings": {
    "gecko": {
      "id": "{f3c1d626-d843-40aa-b90b-70eaec44af7c}"
    }
  }
}